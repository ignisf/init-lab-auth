<li>
  <%= form_for [:user, @computer], remote: true do |f| %>
    <div class="input-with-icon inline-form">
        <i class="ss-icon ss-wifi"></i>
        <%= f.submit "Запиши", class: 'button' %>
        <%= f.text_field :value, required: true, pattern: '[A-Fa-f0-9]{12}' %>
    </div>
  <% end %>
</li>
